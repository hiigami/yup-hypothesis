{
  "name": "yup-hypothesis",
  "version": "0.8.0",
  "description": "Create random data from a Yup schema.",
  "main": "./lib/cjs/main.js",
  "module": "./lib/cjs/main.js",
  "types": "./lib/cjs/main.d.ts",
  "files": [
    "lib/"
  ],
  "scripts": {
    "lint": "eslint",
    "build": "npx tsc -p tsconfig-cjs.json",
    "test": "jest -i",
    "coverage": "jest -i --coverage",
    "prepublishOnly": "npm run build",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hiigami/yup-hypothesis.git"
  },
  "author": "@hiigami",
  "license": "MIT",
  "keywords": [
    "data",
    "fixtures",
    "generator",
    "hypothesis",
    "mocks",
    "test",
    "yup"
  ],
  "bugs": {
    "url": "https://github.com/hiigami/yup-hypothesis/issues"
  },
  "homepage": "https://github.com/hiigami/yup-hypothesis#readme",
  "dependencies": {
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.7",
    "@types/yup": "^0.29.13",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "jest": "^27.0.0",
    "lint-staged": "^11.2.6",
    "mrm": "^3.0.10",
    "np": "^7.5.0",
    "prettier": "^2.4.1",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  },
  "lint-staged": {
    "./**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write",
      "npm run lint"
    ]
  }
}
